{
  "type": "module",
  "name": "mealplanner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./temp & rm -rf ./build && mkdir ./build & mkdir ./temp && npm run build-directories",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:node": "nodemon ./dist/",
    "start": "echo doh!",
    "start:lambda": "netlify-lambda serve functions",
    "build:lambda": "netlify-lambda build functions",
    "tsc:watch": "npx tsc -c -w",
    "html:watch": "npx chokidar \"./temp/**/*.js\" -c \"./scripts/build-html.sh {path}\"",
    "ts:watch": "npx rollup -c -w",
    "watch": "npm-run-all -s clean -p ts:watch html:watch",
    "serve": "http-server ./build",
    "build": "npm-run-all -s build-directories -p copy-js build-html",
    "build:html": "./scripts/build-html.sh",
    "build-directories": "./scripts/build-directories.sh"
  },
